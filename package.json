{
  "name": "vuenode-fullstack",
  "description": "Vue Node Fullstack",
  "author": "asadazmat@gmail.com",
  "private": true,
  "scripts": {
    "start": "node server",
    "clean": "rimraf dist coverage",
    "dev": "npm run clean && cross-env NODE_ENV=development nodemon",
    "dev:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true nodemon",
    "prod": "cross-env NODE_ENV=production npm run start",
    "prod:tunnel": "cross-env NODE_ENV=production ENABLE_TUNNEL=true npm run start",
    "build:prod": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src server tests",
    "fix": "eslint --ext .js,.vue src server tests --fix",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.js",
    "e2e:watch": "testcafe-live chrome e2e/**/*.e2e-spec.js",
    "e2e:ci": "testcafe all e2e/**/*.e2e-spec.js --app \"yarn dev\" --app-init-delay 4000",
    "postinstall": "npm run build:prod && npm run db",
    "db": "sequelize db:migrate && sequelize db:seed:all"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint-check && npm run lint"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "dependencies": {
    "acl": "0.4.11",
    "axios": "0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-env": "5.2.0",
    "es6-promise": "4.2.5",
    "express": "4.16.3",
    "globby": "8.0.1",
    "helmet": "3.13.0",
    "jsonwebtoken": "8.3.0",
    "lodash": "4.17.11",
    "lru-cache": "4.1.3",
    "morgan": "1.9.1",
    "multer": "1.3.1",
    "nodemailer": "4.6.8",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-github": "1.1.0",
    "passport-google-oauth": "1.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "route-cache": "0.4.4",
    "sanitize.css": "7.0.3",
    "sequelize": "4.39.0",
    "sequelize-cli": "4.1.1",
    "serialize-javascript": "1.5.0",
    "serve-favicon": "2.5.0",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1",
    "sqlite3": "4.0.2",
    "vee-validate": "2.1.0-beta.9",
    "vue": "2.5.17",
    "vue-router": "3.0.1",
    "vue-server-renderer": "2.5.17",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@vue/test-utils": "1.0.0-beta.25",
    "autoprefixer": "9.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "chokidar": "2.0.4",
    "css-loader": "1.0.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "4.7.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "husky": "1.0.0-rc.15",
    "image-webpack-loader": "4.3.1",
    "ip": "1.1.5",
    "jest": "23.6.0",
    "jest-serializer-vue": "2.0.2",
    "ngrok": "3.0.1",
    "node-sass": "4.9.3",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "testcafe": "0.22.0",
    "testcafe-live": "0.1.3",
    "url-loader": "1.1.1",
    "vue-jest": "2.6.0",
    "vue-loader": "15.4.2",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.20.0",
    "webpack-cli": "3.1.1",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.2",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  }
}
